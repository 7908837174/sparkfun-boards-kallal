{
  "autorouter": {
    "maxIterations": 500000,
    "timeout": 3600,
    "minViaSpacing": 0.6,
    "minTraceSpacing": 0.5,
    "maxCandidates": 5000,
    "allowIterationWarnings": true,
    "solverType": "hybrid",
    "routingStrategy": "simple"
  },
  "build": {
    "ignoreErrors": true,
    "outputFormat": "json"
  }
}
