{
  "autorouter": {
    "maxIterations": 500000,
    "timeout": 3600,
    "minViaSpacing": 0.4,
    "minTraceSpacing": 0.3,
    "maxCandidates": 10000,
    "allowIterationWarnings": true,
    "solverType": "hybrid",
    "routingStrategy": "simple",
    "relaxConstraints": true,
    "enableMultipass": true
  },
  "build": {
    "ignoreErrors": true,
    "outputFormat": "json"
  }
}